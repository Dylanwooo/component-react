@import "./common/var.styl";

@font-face {
  font-family: 'jdesign-icons';
  src: url( $font-url /jdesign.woff) format('woff'), /* chrome, firefox */
          url($font-url /jdesign.tff) format('truetype'); /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  font-weight: normal;
  font-style: normal
}

[class^="jd-icon-"], [class*="jd-icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'jdesign-icons' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  vertical-align: baseline;
  display: inline-block;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

